{
  "coin": {
    "name": "BSC",
    "shortcut": "BSC",
    "label": "BSC",
    "alias": "bsc"
  },
  "ports": {
    "backend_rpc": 8037,
    "backend_message_queue": 0,
    "backend_p2p": 38337,
    "backend_http": 8137,
    "blockbook_internal": 9037,
    "blockbook_public": 9137
  },
  "ipc": {
    "rpc_url_template": "ws://127.0.0.1:{{.Ports.BackendRPC}}",
    "rpc_timeout": 25
  },
  "backend": {
    "package_name": "backend-bsc",
    "package_revision": "satoshilabs-1",
    "system_user": "bsc",
    "version": "1.9.13-cbc4ac26",
    "binary_url": "https://github.com/binance-chain/bsc/releases/download/v1.0.6/mainnet.zip",
    "extract_command": "wget https://github.com/binance-chain/bsc/releases/download/v1.0.6/geth_linux && mv geth_linux ./backend && chmod +x ./backend/geth_linux && unzip -d backend",
    "exec_command_template": "/bin/sh -c 'if [ ! -f \"{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/geth/nodekey\" ]; then {{.Env.BackendInstallPath}}/{{.Coin.Alias}}/geth_linux --datadir {{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend init ./genesis.json; fi && {{.Env.BackendInstallPath}}/{{.Coin.Alias}}/geth_linux --config ./config.toml --ipcdisable --syncmode full --cache 1024 --nat none --pprof --pprofaddr 0.0.0.0 --metrics --datadir {{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend --port 38335 --ws --wsaddr 127.0.0.1 --wsport {{.Ports.BackendRPC}} --wsorigins \"*\" --rpc --rpcport 8135 -rpcaddr 127.0.0.1 --rpccorsdomain \"*\" --rpcvhosts \"*\" 2>>{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/node.log'",
    "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/{{.Coin.Alias}}.log",
    "postinst_script_template": "",
    "service_type": "simple",
    "service_additional_params_template": "",
    "protect_memory": true,
    "mainnet": true,
    "server_config_file": "",
    "client_config_file": ""
  },
  "blockbook": {
    "package_name": "blockbook-bsc",
    "system_user": "blockbook-bsc",
    "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
    "public_binding_template": ":{{.Ports.BlockbookPublic}}",
    "explorer_url": "",
    "additional_params": "",
    "block_chain": {
      "parse": true,
      "mempool_workers": 8,
      "mempool_sub_workers": 2,
      "block_addresses_to_keep": 300,
      "additional_params": {
        "mempoolTxTimeoutHours": 48,
        "queryBackendOnMempoolResync": false,
        "fiat_rates": "coingecko",
        "fiat_rates_params": "{\"url\": \"https://api.coingecko.com/api/v3\", \"coin\": \"ethereum\", \"periodSeconds\": 60}"
      }
    }
  },
  "meta": {
    "package_maintainer": "IT",
    "package_maintainer_email": "tom.zhao@binance.com"
  }
}
